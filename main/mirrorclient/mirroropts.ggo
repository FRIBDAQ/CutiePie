package "Mirror"
version "1.0"
purpose "Mirror SpecTcl shared display memory"

option "host" H "Host SpecTcl is running in" string required
option "restport" R "Port number or DAQPortManager Service for SpecTcl REST service" string required
option "mirrorport" M "Port number or DAQPortManager service for SpecTcl Mirror service" string required
